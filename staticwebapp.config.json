{
    "auth": {
      "identityProviders": {
        "customOpenIdConnectProviders": {
          "google": {
            "registration": {
              "clientIdSettingName": "GOOGLE_CLIENT_ID",
              "clientSecretSettingName": "GOOGLE_CLIENT_SECRET",
              "authority": "https://accounts.google.com",
              "scopes": ["openid", "profile", "email"]
            },
            "login": {
              "nameClaimType": "email",
              "roleClaimType": "roles"
            }
          }
        }
      }
    },
"routes": [
    {
      "route": "/",
      "allowedRoles": []  // No roles required for home page
    },
    {
      "route": "/profile",
      "allowedRoles": ["authenticated"]  // Only authenticated users can access the profile page
    },
    {
      "route": "/dashboard",
      "allowedRoles": ["authenticated"]  // Only authenticated users can access the dashboard
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/google"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
  ]
  }
  