{
    "auth": {
      "identityProviders": {
        "google": {
          "registration": {
            "clientIdSettingName": "GOOGLE_CLIENT_ID",
            "clientSecretSettingName": "GOOGLE_CLIENT_SECRET"
          }
        }
      }
    }
  ,

"routes": [
    {
        "route": "/",
        "allowedRoles": []
    },
    {
        "route": "/welcome",
        "allowedRoles": ["authenticated"]
    },
    {
        "route": "/login",
        "redirect": "/.auth/login/google"
    },
    {
        "route": "/.auth/complete",
        "redirect": "/welcome"
    },
    {
        "route": "/logout",
        "redirect": "/.auth/logout"
    }
  ]
}

