{
    "auth": {
      "identityProviders": {
        "google": {
          "registration": {
            "clientIdSettingName": "GOOGLE_CLIENT_ID",
            "clientSecretSettingName": "GOOGLE_CLIENT_SECRET"
          }
        }
      }
    }
  ,

"routes": [
    {
      "route": "/",
      "allowedRoles": [],
      "redirect": "/welcome" 
    },
    {
      "route": "/login",
      "allowedRoles": [],
      "redirect": "/welcome" 
    },
    {
        "route": "/welcome",
        "allowedRoles": ["authenticated"],
        "redirect": "/login" 
    },
    {
        "route": "/.auth/login/google",
        "redirect": "/welcome"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    }
  ]
}

